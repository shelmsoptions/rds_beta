<!DOCTYPE html>
<html>
  <head>
    <title>RdsBeta</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if session[:user_id] %>
      <a href="/sessions/<%= session[:user_id] %>" data-method="delete">Logout</a>
    <% end %>
    <%= yield %>
  </body>
</html>
